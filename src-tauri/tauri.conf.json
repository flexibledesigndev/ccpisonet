{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "cc-pisonet",
  "version": "0.1.0",
  "identifier": "cc.pisonet",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "decorations": false,
        "transparent": true,
        "center": true,
        "resizable": false,
        "title": "Cara & Casey Pisonet",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
